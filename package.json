{
  "version": "1.0.0",
  "license": "MIT",
  "public": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack serve --config ./config/webpack.dev.ts",
    "build": "webpack --mode=production --config ./config/webpack.prod.ts",
    "build-batch": "webpack --mode=production --config ./config/webpack.prod.batch.ts",
    "analyze": "webpack --analyze --config ./config/webpack.prod.ts",
    "watch": "webpack --progress --watch",
    "tsc": "tsc -p tsconfig.json",
    "lint": "eslint --fix --ext .js,.tsx,.ts src/",
    "stylelint": "stylelint --fix .less src/",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,less,md}\""
  },
  "lint-staged": {
    "**/*.less": "npm run stylelint",
    "**/*.{js,jsx,ts,tsx}": "npm run lint",
    "**/*.{js,jsx,tsx,ts,less,md,json}": "npm run prettier"
  },
