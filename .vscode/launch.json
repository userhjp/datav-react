{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug TS",
      "request": "launch",
      "type": "node",
      "runtimeArgs": [
        "-r",
        "${workspaceFolder}/node_modules/ts-node/register",
      ],
      // "args": ["${workspaceFolder}/config/devServer.ts"],
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_PROJECT": "${workspaceFolder}/tsconfig.json"
      },
      "outFiles": ["${workspaceFolder}/lib/**/*.js"]
    },

  ]
}
